<% if current_user  %>

<%= link_to 'Add new Gif!', new_gif_path %>

<h2>
<% @gifs.each do |gif| %>
  <ul>
    <img class="image" src="<%= gif.image %>">
  </ul>
<% end %>
</h2>

<% else %>

<h2>
<% @gifs.each do |gif| %>
  <ul>
    <img class="image" src="<%= gif.image %>">
  </ul>
<% end %>
</h2>
<% end %>
